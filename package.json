{
  "name": "meist",
  "version": "1.0.0",
  "description": "An issue tracker based on on mern",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -w server.js server.js",
    "compile:release": "grunt release",
    "compile": "babel src --presets react,es2015 --out-dir static",
    "pretest": "npm run compile && npm run lint && npm run compile:release",
    "watch": "babel src --presets react,es2015 --out-dir static --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json '**/*.js'",
    "lint:fix": "npm run lint --fix"
  },
  "pre-commit": [
    "compile",
    "lint"
  ],
  "author": "acellam guy",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.2",
    "swagger-ui-express": "^2.0.13"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "grunt": "1.0.1",
    "grunt-check-dependencies": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^20.1.0",
    "nodemon": "^1.14.6",
    "pre-commit": "^1.2.2"
  }
}
